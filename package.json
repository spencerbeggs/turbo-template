{
	"name": "turbo-template",
	"version": "0.0.0",
	"description": "A nicely configured starting point for projects configured with Turborepo.",
	"packageManager": "pnpm@8.13.1",
	"license": "MIT",
	"homepage": "https://github.com/spencerbeggs/turbo-template#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/spencerbeggs/turbo-template.git"
	},
	"author": {
		"name": "C. Spencer Beggs",
		"email": "spencer@beg.gs"
	},
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"bootstrap": "turbo gen ./turbo/generators/config.ts",
		"dev": "turbo run dev --filter=@template/*",
		"dev:templates": "turbo run dev --filter=@template/*",
		"gen": "turbo generate",
		"lint": "turbo run eslint --filter=!@template/* --filter=!plop-* --filter=!@config/*",
		"lint:all": "turbo run eslint",
		"lint-fix": "turbo run eslint-fix --filter=!@template/* --filter=!plop-* --filter=!@config/*",
		"lint-fix:all": "turbo run eslint-fix",
		"publish-packages": "turbo run publish-packages --filter=!@template/* --filter=!plop-* --filter=!@config/*",
		"test": "turbo run test --color --filter=!@template/* --filter=!plop-* --filter=!@config/*",
		"test:all": "turbo run test --color"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.1",
		"@config/eslint": "workspace:*",
		"@config/jest": "workspace:*",
		"@config/tsconfig": "workspace:*",
		"@turbo/gen": "^1.11.2",
		"@plop/bootstrap-repo": "workspace:*",
		"@plop/turbo-pnpm-workspace": "workspace:*",
		"@plop/turbo-vscode-eslint": "workspace:*",
		"turbo": "1.11.2"
	}
}
