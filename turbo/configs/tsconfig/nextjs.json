{
	"include": ["next-env.d.ts", "middleware.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "types/**/*.ts"],
	"exclude": ["node_modules", "dist", "build", ".turbo", ".next"],
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@vercel/analytics/react": ["./node_modules/@vercel/analytics/dist/react"]
		},
		"typeRoots": ["./node_modules/@types", "./types/*.d.ts"]
	}
}
